config_version=5

[application]

config/name="Godot Tiny MMO"
run/main_scene="res://common/main/main.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

ClientEvents="*res://common/autoloads/client_events.gd"
Client="*res://common/autoloads/client.gd"
Server="*res://common/autoloads/server.gd"
DevTool="*res://common/autoloads/dev_tool.gd"

[debug]

gdscript/warnings/unused_signal=0

[display]

window/size/viewport_width=960
window/size/viewport_height=540
window/size/window_width_override=960
window/size/window_height_override=540
window/stretch/mode="canvas_items"

[file_customization]

folder_colors={
"res://assets/": "green",
"res://client/": "blue",
"res://common/": "yellow",
"res://server/": "pink"
}

